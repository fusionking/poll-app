<%= bootstrap_form_for(@question, url: url) do |form| %>
  <%= form.hidden_field :submittable_type %>
  <%= form.text_field :title %>
  <%= render(
    "#{question.submittable.to_partial_path}_form",
    submittable: question.submittable,
    form: form,
    poll: question.poll
  ) %>
  <%= form.submit %>
<% end %>
